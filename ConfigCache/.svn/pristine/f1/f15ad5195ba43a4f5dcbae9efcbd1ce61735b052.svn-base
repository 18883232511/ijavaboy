package org.config.cache.decode.text;

import org.config.cache.StringArray;
import org.config.cache.core.AbstractDecoder;
import org.config.cache.data.MonsterRefreshConfig;

/**
 * 怪物刷新表行解析器
 * @author chenjie
 * 2012-12-13
 */
public class MonsterRefreshDecoder extends AbstractDecoder<MonsterRefreshConfig> {

	@Override
	public MonsterRefreshConfig decode(StringArray values) {
		MonsterRefreshConfig config = new MonsterRefreshConfig();
		config.fromStringArray(values);
		return config;
	}

}
