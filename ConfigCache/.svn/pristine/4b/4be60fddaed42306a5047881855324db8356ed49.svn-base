package org.config.cache.decode.text;

import org.config.cache.StringArray;
import org.config.cache.core.AbstractDecoder;
import org.config.cache.data.MissionConfig;

/**
 * 任务配置表行解析器
 * @author chenjie
 * 2012-12-12
 */
public class MissionDecoder extends AbstractDecoder<MissionConfig> {

	@Override
	public MissionConfig decode(StringArray values) {
		MissionConfig config = new MissionConfig();
		config.fromStringArray(values);
		return config;
	}

}
