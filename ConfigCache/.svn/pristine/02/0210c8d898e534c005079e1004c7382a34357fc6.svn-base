package org.config.cache.decode.text;

import org.config.cache.StringArray;
import org.config.cache.core.AbstractDecoder;
import org.config.cache.data.ShopItemConfig;

/**
 * 商品配置表行解析器
 * @author chenjie
 * 2012-12-12
 */
public class ShopItemDecoder extends AbstractDecoder<ShopItemConfig> {

	@Override
	public ShopItemConfig decode(StringArray values) {
		ShopItemConfig config = new ShopItemConfig();
		config.fromStringArray(values);
		return config;
	}

}
