package org.config.cache.decode.text;

import org.config.cache.StringArray;
import org.config.cache.core.AbstractDecoder;
import org.config.cache.data.RoleLevelConfig;

/**
 * 玩家角色配置表行解析器
 * @author chenjie
 * 2012-12-12
 */
public class RoleLevelDecoder extends AbstractDecoder<RoleLevelConfig> {

	@Override
	public RoleLevelConfig decode(StringArray values) {
		RoleLevelConfig config = new RoleLevelConfig();
		config.fromStringArray(values);
		return config;
	}

}
