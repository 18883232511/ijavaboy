package org.config.cache.decode.text;

import org.config.cache.StringArray;
import org.config.cache.core.AbstractDecoder;
import org.config.cache.data.MonsterGroupConfig;

/**
 * 怪物组配置表行解析器
 * @author chenjie
 * 2012-12-13
 */
public class MonsterGroupDecoder extends AbstractDecoder<MonsterGroupConfig> {

	@Override
	public MonsterGroupConfig decode(StringArray values) {
		MonsterGroupConfig config = new MonsterGroupConfig();
		config.fromStringArray(values);
		return config;
	}

}
